@model IEnumerable<BALibrary.Registrar.MarkSheetDetail>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Registrar/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ExamGroupDetailId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExamMark)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsPassed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MarkSheet)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ExamGroupDetailId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExamMark)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsPassed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MarkSheet.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
